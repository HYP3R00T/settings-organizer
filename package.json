{
    "name": "settings-organizer",
    "displayName": "Settings Organizer",
    "description": "Automatically organize and tidy up your VSCode settings.json file.",
    "version": "0.0.24",
    "publisher": "hyperoot",
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/HYP3R00T/settings-organizer.git"
    },
    "icon": "images/logo.png",
    "engines": {
        "vscode": "^1.91.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.settings-organizer",
                "title": "Settings Organizer"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "vscode-test",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@types/mocha": "^10.0.10",
        "@types/node": "~22.14.1",
        "@types/vscode": "^1.102.0",
        "@typescript-eslint/eslint-plugin": "^8.36.0",
        "@typescript-eslint/parser": "^8.36.0",
        "@vscode/test-cli": "^0.0.10",
        "@vscode/test-electron": "^2.5.2",
        "eslint": "^9.31.0",
        "prettier": "^3.6.2",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "comment-json": "^4.2.5"
    },
    "packageManager": "pnpm@10.6.1+sha512.40ee09af407fa9fbb5fbfb8e1cb40fbb74c0af0c3e10e9224d7b53c7658528615b2c92450e74cfad91e3a2dcafe3ce4050d80bda71d757756d2ce2b66213e9a3"
}
